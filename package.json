{
  "name": "electron-quick-start-typescript",
  "version": "1.0.0",
  "description": "A minimal Electron application written with Typescript",
  "scripts": {
    "electron:build": "tsc -p ./electron",
    "electron:watch": "tsc -w -p ./electron",
    "electron:lint": "tslint -c tslint.json -p tsconfig.json",
    "electron:test": "jest -c ./electron/jest.config.js",
    "electron:start": "npm run electron:build && electron ./electron/dist/electron/src/main.js",
    "start": "rm tsconfig.json && react-scripts start",
    "build": "rm tsconfig.json && react-scripts build",
    "test": "rm tsconfig.json && react-scripts test",
    "eject": "rm tsconfig.json && react-scripts eject"
  },
  "repository": "https://github.com/electron/electron-quick-start-typescript",
  "keywords": [
    "Electron",
    "quick",
    "start",
    "tutorial",
    "demo",
    "typescript"
  ],
  "author": "GitHub",
  "license": "CC0-1.0",
  "dependencies": {
    "@fullpage/react-fullpage": "^0.1.9",
    "color": "^3.1.0",
    "electron-react-titlebar": "^0.8.1",
    "iohook": "^0.2.3",
    "lodash": "^4.17.11",
    "react": "^16.7.0-alpha.0",
    "react-circular-progressbar": "^1.0.0",
    "react-dom": "^16.7.0-alpha.0",
    "react-icons": "^3.2.2",
    "react-router-dom": "^4.3.1",
    "rxjs": "^6.3.3",
    "styled-components": "^4.0.3"
  },
  "devDependencies": {
    "@types/color": "^3.0.0",
    "@types/fullpage.js": "^2.9.0",
    "@types/jest": "^23.3.9",
    "@types/lodash.isnumber": "^3.0.4",
    "@types/react": "^16.7.13",
    "@types/react-circular-progressbar": "^1.0.1",
    "@types/react-dom": "^16.0.9",
    "@types/react-router-dom": "^4.3.1",
    "@types/styled-components": "^4.0.3",
    "electron": "^2.0.2",
    "jest": "^23.6.0",
    "react-scripts": "2.1.1",
    "ts-jest": "^23.10.4",
    "tslint": "^5.10.0",
    "typescript": "^2.9.2"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "iohook": {
    "targets": [
      "node-59",
      "electron-57"
    ],
    "platforms": [
      "win32",
      "darwin",
      "linux"
    ],
    "arches": [
      "x64",
      "ia32"
    ]
  },
  "build": {
    "appId": "ahmed.magdy.cheesytimer",
    "mac": {}
  }
}

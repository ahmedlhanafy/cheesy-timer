{
  "name": "electron-quick-start-typescript",
  "version": "1.0.0",
  "description": "A minimal Electron application written with Typescript",
  "scripts": {
    "electron:build": "tsc -p ./electron",
    "electron:watch": "tsc -w -p ./electron",
    "electron:lint": "tslint -c tslint.json -p tsconfig.json",
    "electron:start": "npm run electron:build && electron ./electron/dist/main.js",
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "repository": "https://github.com/electron/electron-quick-start-typescript",
  "keywords": [
    "Electron",
    "quick",
    "start",
    "tutorial",
    "demo",
    "typescript"
  ],
  "author": "GitHub",
  "license": "CC0-1.0",
  "dependencies": {
    "active-win": "^4.0.0",
    "iohook": "^0.2.3",
    "lodash": "^4.17.11",
    "lodash.isnumber": "^3.0.3",
    "react": "^16.7.0-alpha.0",
    "react-circular-progressbar": "^1.0.0",
    "react-dom": "^16.7.0-alpha.0",
    "react-icons": "^3.2.2",
    "react-scroll-paged-view": "^2.2.5",
    "rxjs": "^6.3.3",
    "styled-components": "^4.0.3"
  },
  "devDependencies": {
    "@types/lodash.isnumber": "^3.0.4",
    "@types/react": "^16.4.18",
    "@types/react-circular-progressbar": "^1.0.1",
    "@types/react-dom": "^16.0.9",
    "@types/styled-components": "^4.0.3",
    "electron": "^2.0.2",
    "react-scripts": "2.1.1",
    "tslint": "^5.10.0",
    "typescript": "^2.8.3"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "iohook": {
    "targets": [
      "node-59",
      "electron-57"
    ],
    "platforms": [
      "win32",
      "darwin",
      "linux"
    ],
    "arches": [
      "x64",
      "ia32"
    ]
  },
  "build": {
    "appId": "ahmed.magdy.cheesytimer",
    "mac": {}
  }
}
